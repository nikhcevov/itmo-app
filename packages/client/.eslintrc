{
    "extends": ["airbnb", "airbnb/hooks"],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/no-array-index-key": 0,
        "jsx-quotes": ["error", "prefer-single"]
    },
    "globals": {
        "document": false,
        "window": false
    }
}